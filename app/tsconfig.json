{
  "extends": "@vonsim/tsconfig/react.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@vonsim/common": ["../packages/common/src"],
      "@vonsim/common/*": ["../packages/common/src/*"],
      "@vonsim/simulator": ["../packages/simulator/src"],
      "@vonsim/simulator/*": ["../packages/simulator/src/*"],
      "@vonsim/assembler": ["../packages/assembler/src"],
      "@vonsim/assembler/*": ["../packages/assembler/src/*"]
    },
    "types": ["wicg-file-system-access"]
  },
  "include": ["src"]
}
